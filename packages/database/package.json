{
  "name": "@onlineusta/database",
  "version": "0.1.0",
  "description": "Database schemas and utilities for OnlineUsta",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "prisma": {
    "seed": "tsx prisma/seed-marketplace.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "prisma": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@onlineusta/eslint-config": "workspace:*",
    "@onlineusta/typescript-config": "workspace:*",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
